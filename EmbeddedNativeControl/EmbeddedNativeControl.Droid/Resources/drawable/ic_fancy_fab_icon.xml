<?xml version="1.0" encoding="UTF-8" ?>
<animated-selector xmlns:android="http://schemas.android.com/apk/res/android" android:constantSize="true">
	<item android:id="@+id/fab_checked_state" android:state_checked="true" android:drawable="@drawable/ic_door" />
	<item android:id="@+id/fab_unchecked_state" android:drawable="@drawable/ic_finger_buzzer" />

	<transition android:fromId="@+id/fab_unchecked_state" android:toId="@+id/fab_checked_state" android:drawable="@drawable/anim_vector_finger_to_door" />
</animated-selector>
